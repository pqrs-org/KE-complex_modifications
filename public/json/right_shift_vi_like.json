{
  "title": "right shift vi like",
  "rules": [
    {
      "description": "Right (right_shift+l to right)",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_shift"],
              "optional": ["any"]

            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Down (right_shift+j to down)",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Left (right_shift+h to left)",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
