{
  "title": "Exchange square brackets and curly brackets",
  "rules": [
    {
      "description": "Swap {} and []",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket"
          },
          "to": [
            {
              "modifiers": ["left_shift"],
              "key_code": "open_bracket"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["left_shift"]
            }
          },
          "to": [
            {
              "key_code": "open_bracket"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket"
          },
          "to": [
            {
              "modifiers": ["left_shift"],
              "key_code": "close_bracket"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": ["left_shift"]
            }
          },
          "to": [
            {
              "key_code": "close_bracket"
            }
          ]
        }
      ]
    }
  ]
}
