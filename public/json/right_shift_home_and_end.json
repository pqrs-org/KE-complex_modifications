                      "^com\\.googlecode\\.iterm2$",
                      "^co\\.zeit\\.hyperterm$",
                      "^co\\.zeit\\.hyper$",
                      "^io\\.alacritty$",
                      "^net\\.kovidgoyal\\.kitty$"
                    ],
                    "type": "frontmost_application_unless"
                  }
                ],
                "type": "basic",
                "from": {
                  "key_code": "end",
                  "modifiers": {
                    "mandatory": [
                      "right_shift"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "right_arrow",
