{
    "description": "Remap F18 to Control+Shift+P useful for SnagIt or example simple remap of F13-Fnn",
    "manipulators": [
        {
            "from": {
                "key_code": "f18",
                "modifiers": { "optional": ["any"] }
            },
            "to": [
                {
                    "key_code": "p",
                    "modifiers": ["left_control", "left_shift"]
                }
            ],
            "type": "basic"
        }
    ]
}
