{
    "title": "Swiss Keyboard Layout for macOS",
    "rules": [
        {
            "description": "BASIC MODE",
            "manipulators": [
                {
                    "from": {
                        "key_code": "quote",
                        "modifiers": {
                            "mandatory": ["shift"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "8",
                            "modifiers": ["right_option"]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "non_us_pound",
                        "modifiers": {
                            "mandatory": ["shift"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": ["right_option"]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "backslash",
                        "modifiers": {
                            "mandatory": ["shift"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "9",
                            "modifiers": ["right_option"]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "open_bracket",
                        "modifiers": {
                            "mandatory": ["shift"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "5",
                            "modifiers": ["right_option"]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": {
                        "key_code": "close_bracket",
                        "modifiers": {
                            "mandatory": ["shift"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "6",
                            "modifiers": ["right_option"]
                        }
                    ],
                    "type": "basic"
                },
                {
                    "from": { "key_code": "close_bracket" },
                    "to": [
                        {
                            "key_code": "close_bracket",
                            "modifiers": ["left_shift"]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
