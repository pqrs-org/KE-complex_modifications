{
  "title": "Windows-like curly bracket key combination for French-AZERTY layout",
  "rules": [{
      "description": "Right command and 4 for {",
      "manipulators": [{
        "from": {
          "key_code": "4",
          "modifiers": {
            "mandatory": [
              "right_command"
            ]
          }
        },
        "to": {
          "key_code": "5",
          "modifiers": [
            "right_option"
          ]
        },
        "type": "basic"
      }]
    },
    {
      "description": "Right command and -_ for }",
      "manipulators": [{
        "from": {
          "key_code": "equal_sign",
          "modifiers": {
            "mandatory": [
              "right_command"
            ]
          }
        },
        "to": {
          "key_code": "hyphen",
          "modifiers": [
            "right_option"
          ]
        },
        "type": "basic"
      }]
    }
  ]
}
