{
  "title": "Dictation to Mutify",
  "rules": [
    {
      "description": "Dictation key (F5 action) to trigger Mutify mute/unmute (⇧⌘0) action. Most useful for Macs without Touchbar.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}