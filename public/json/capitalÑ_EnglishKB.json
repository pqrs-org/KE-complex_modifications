{
    "title": "Spanish characters: [Ñ] (capital ñ shortcut)",
    "maintainers": ["ticodb"],
    "rules": [
        {
            "description": "Add capital N with sign (Ñ) using Control + Shift + N",
            "manipulators": [
                {
                    "from": {
                        "key_code": "n",
                        "modifiers": { "mandatory": ["control", "shift"] }
                    },
                    "to": [
                        {
                            "key_code": "n",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "n",
                            "modifiers": ["shift"]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
