{
	"title": "Hai HHKB Launch apps (Brave, VS Code, Signal, Slack, Postman, Sublime Merge, iTerm, Thunderbird, iMessage, Mail, Draw.IO, Sequel Ace)",
	"rules": [
		{
			"description": "right_option + D(Database) -> Sequal Ace",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "d",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/Applications/Sequel Ace.app'"
						}
					]
				}
			]
		},
