{
	"title": "Hai HHKB Launch apps (Brave, VS Code, Signal, Slack, Postman, Sublime Merge, iTerm)",
	"rules": [
		{
			"description": "right_option + B -> Brave",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "b",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/Applications/Brave Browser.app'"
						}
					]
				}
			]
		},
		{
			"description": "right_option + V -> VS code",
			"manipulators": [
