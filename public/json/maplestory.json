		{
			"description": "When MapleStory focused, map fn + return to Insert",
			"manipulators": [
				{
					"conditions": [
						{
							"bundle_identifiers": [
								"^com\\.nexon\\.maplestoryna_exe$"
							],
							"type": "frontmost_application_if"
						}
					],
					"from": {
						"key_code": "return_or_enter",
						"modifiers": { "mandatory": ["fn"] }
					},
					"to": [{ "key_code": "insert" }],
					"type": "basic"
				}
			]
		},
