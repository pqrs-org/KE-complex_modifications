{
    "title": "Spanish Keyboard Vowels Bundle",
    "maintainers": ["ticodb"],
    "rules": [
                {
            "description": "Add lowercase vowels with accent mark (Control + vowel)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "a"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "e"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "i"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "o"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": ["control"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "u"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Add uppercase vowels with accent mark (Control + Shift + vowel)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "a",
                        "modifiers": {
                            "mandatory": ["control", "shift"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "a",
                            "modifiers": ["shift"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": ["control", "shift"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "e",
                            "modifiers": ["shift"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": ["control", "shift"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "i",
                            "modifiers": ["shift"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": ["control", "shift"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "o",
                            "modifiers": ["shift"]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": ["control", "shift"]
                        }
                    },
                    "to": [
                        {
                            "key_code": "e",
                            "modifiers": ["option"]
                        },
                        {
                            "key_code": "u",
                            "modifiers": ["shift"]
                        }
                    ]
                }
            ]
        }
    ]
}
