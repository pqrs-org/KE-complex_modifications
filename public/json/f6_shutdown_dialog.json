{
    "title": "Maps F6 to Ctl + Eject (Shutdown dialog)",
    "rules": [
        {
            "description": "F6 to Ctl + Eject",
            "manipulators": [
                {
                    "from": {
                        "key_code": "f6",
                        "modifiers": {
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "consumer_key_code": "eject",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
