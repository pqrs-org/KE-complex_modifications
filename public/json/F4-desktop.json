{
  "title": "Show Desktop with F4",
  "rules": [
    {
      "description": "Show Desktop with F4",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f4"
          },
          "to": [
            {
              "shell_command": "/'System/Applications/Mission Control.app/Contents/MacOS/Mission Control' 1"
            }
          ]
        }
      ]
    }
  ]
}
