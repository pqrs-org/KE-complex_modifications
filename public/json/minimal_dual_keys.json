{
  "title": "Minimal dual keys",
  "maintainers": [
    "yeonsh"
  ],
  "rules": [
    {
      "description": "Minimal dual keys",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_command"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 300,
            "basic.to_if_held_down_threshold_milliseconds": 0
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "right_command"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 300,
            "basic.to_if_held_down_threshold_milliseconds": 0
          }
        },
        {
          "type": "basic",
          "from": {
            "key_code": "left_shift",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_shift"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 300,
            "basic.to_if_held_down_threshold_milliseconds": 0
          }
        }
      ]
    }
  ]
}
