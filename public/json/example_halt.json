{
  "title": "halt example",
  "rules": [
    {
      "description": "Open Mission Control by hold tab key",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab"
          },
          "to_after_key_up": [
            {
              "key_code": "tab"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "mission_control",
              "halt": true
            }
          ]
        }
      ]
    }
  ]
}
