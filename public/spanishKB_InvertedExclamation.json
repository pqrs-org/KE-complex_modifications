{
  "title": "Spanish punctuation: [ยก] (inverted exclamation)",
  "rules": [
    {
      "description": "Add inverted exclamation mark (ยก) with Control + 1",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "1",
              "modifiers": [
                "option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
