{
	"title": "US keyboard with Croatian letters on right command key",
	"rules": [
		{
			"description": "right_command + '[' = 'š'",
			"manipulators": [
				{
					"type": "basic",
					"conditions": [
						{
							"type": "input_source_if",
							"input_sources": [
								{
									"input_source_id": "com.apple.keylayout.US"
								}
							]
						}
					],
					"from": {
						"key_code": "open_bracket",
						"modifiers": {
							"mandatory": [
								"right_command"
							],
							"optional": [
								"shift",
								"caps_lock"
							]
						}
					},
					"to": [
						{
							"select_input_source": {
								"input_source_id": "com.apple.keylayout.Croatian"
							}
						}
					],
					"to_after_key_up": [
						{
							"key_code": "open_bracket"
						},
						{
							"select_input_source": {
								"input_source_id": "com.apple.keylayout.US"
							}
						}
					]
				}
			]
		},
