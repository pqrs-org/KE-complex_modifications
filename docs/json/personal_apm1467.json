{
  "title": "Personal rules (@apm1467)",
  "rules": [
    {
      "description": "Change right_option to command + option + shift",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "right_option",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_shift",
              "modifiers": [
                "left_option",
                "left_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
