                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": ["left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {"key_code": "s"},
                        {"key_code": "m"}
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {"key_code": "s"},
                        {"key_code": "n"}
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": ["left_control", "fn"],
