        },
        {
            "description": "Change Shift + Left_control + Fn to Shift + S",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "modifiers": {
                            "mandatory": ["left_shift", "left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [{
                        "key_code": "s",
                        "modifiers": ["left_shift"]
                    }]
                },
                {
                    "type": "basic",
                    "from": {
                        "modifiers": {
                            "mandatory": ["right_shift", "left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [{
                        "key_code": "s",
                        "modifiers": ["right_shift"]
                    }]
                }
            ]
        }
    ]
}
