                        "modifiers": {
                            "mandatory": ["left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {"key_code": "s"},
                        {"key_code": "c"}
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "d",
                        "modifiers": {
                            "mandatory": ["left_control", "fn"],
                            "optional": ["any"]
                        }
                    },
                    "to": [
                        {"key_code": "s"},
                        {"key_code": "d"}
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
