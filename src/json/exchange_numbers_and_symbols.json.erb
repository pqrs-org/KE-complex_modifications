{
    "title": "Exchange numbers and symbols",
    "rules": [
        {
            "description": "Exchange numbers and symbols (1234567890 and !@#$%^&*())",
            "manipulators": [
                { "type": "basic", "from": <%= from("1", [], ["caps_lock"]) %>, "to": <%= to([["1", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("2", [], ["caps_lock"]) %>, "to": <%= to([["2", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("3", [], ["caps_lock"]) %>, "to": <%= to([["3", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("4", [], ["caps_lock"]) %>, "to": <%= to([["4", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("5", [], ["caps_lock"]) %>, "to": <%= to([["5", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("6", [], ["caps_lock"]) %>, "to": <%= to([["6", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("7", [], ["caps_lock"]) %>, "to": <%= to([["7", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("8", [], ["caps_lock"]) %>, "to": <%= to([["8", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("9", [], ["caps_lock"]) %>, "to": <%= to([["9", ["left_shift"]]]) %> },
                { "type": "basic", "from": <%= from("0", [], ["caps_lock"]) %>, "to": <%= to([["0", ["left_shift"]]]) %> },

                { "type": "basic", "from": <%= from("1", ["shift"], ["caps_lock"]) %>, "to": <%= to([["1"]]) %> },
                { "type": "basic", "from": <%= from("2", ["shift"], ["caps_lock"]) %>, "to": <%= to([["2"]]) %> },
                { "type": "basic", "from": <%= from("3", ["shift"], ["caps_lock"]) %>, "to": <%= to([["3"]]) %> },
                { "type": "basic", "from": <%= from("4", ["shift"], ["caps_lock"]) %>, "to": <%= to([["4"]]) %> },
                { "type": "basic", "from": <%= from("5", ["shift"], ["caps_lock"]) %>, "to": <%= to([["5"]]) %> },
                { "type": "basic", "from": <%= from("6", ["shift"], ["caps_lock"]) %>, "to": <%= to([["6"]]) %> },
                { "type": "basic", "from": <%= from("7", ["shift"], ["caps_lock"]) %>, "to": <%= to([["7"]]) %> },
                { "type": "basic", "from": <%= from("8", ["shift"], ["caps_lock"]) %>, "to": <%= to([["8"]]) %> },
                { "type": "basic", "from": <%= from("9", ["shift"], ["caps_lock"]) %>, "to": <%= to([["9"]]) %> },
                { "type": "basic", "from": <%= from("0", ["shift"], ["caps_lock"]) %>, "to": <%= to([["0"]]) %> }
            ]
        }
    ]
}
