{
    "title": "Personal rules (@apm1467)",
    "rules": [
        {
            "description": "Disable command + Q in web browser",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("q", ["command"], ["caps_lock"]) %>,
                    "to": <%= to([["vk_none"]]) %>,
                    "conditions": [ <%= frontmost_application_if("browser") %> ]
                }
            ]
        },
        {
            "description": "Change right_option to command + option + shift",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("right_option", [], ["caps_lock"]) %>,
                    "to": <%= to([["left_shift", ["left_option", "left_command"]]]) %>
                }
            ]
        }
    ]
}
