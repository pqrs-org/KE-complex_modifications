{
    "title": "Exchange semicolon and colon",
    "rules": [
        {
            "description": "Exchange semicolon and colon",
            "manipulators": [
                { "type": "basic", "from": <%= from("semicolon", [], ["caps_lock"]) %>, "to": <%= to([["semicolon", ["left_shift"]]]) %> },

                { "type": "basic", "from": <%= from("semicolon", ["shift"], ["caps_lock"]) %>, "to": <%= to([["semicolon"]]) %> }
            ]
        }
    ]
}
