{
    "title": "PC-Style Shortcuts",
    "rules": [
        {
            "description": "Home key to the beginning of the line (Control + a)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("home", [], ["any"]) %>,
                    "to": <%= to([["a", ["left_control"]]]) %>
                }
            ]
        },
        {
            "description": "End key to the end of the line (Control + e)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("end", [], ["any"]) %>,
                    "to": <%= to([["e", ["left_control"]]]) %>
                }
            ]
        },
        {
            "description": "Home key to the beginning of the sentence (Command + Left). Doesnt work in terminal",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("home", [], ["any"]) %>,
                    "to": <%= to([["left_arrow", ["left_command"]]]) %>
                }
            ]
        },
        {
            "description": "End key to the end of the sentence (Command + Right). Doesnt work in terminal",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("end", [], ["any"]) %>,
                    "to": <%= to([["right_arrow", ["left_command"]]]) %>
                }
            ]
        },
        {
            "description": "PC-Style Home/End",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("home", [], ["shift"]) %>,
                    "to": <%= to([["left_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "browser", "git_gui", "jetbrains_ide", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("home", [], ["shift"]) %>,
                    "to": <%= to([["a", ["left_control"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("home", ["control"], ["shift"]) %>,
                    "to": <%= to([["up_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "git_gui", "jetbrains_ide", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("end", [], ["shift"]) %>,
                    "to": <%= to([["right_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "browser", "git_gui", "jetbrains_ide", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("end", [], ["shift"]) %>,
                    "to": <%= to([["e", ["left_control"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("end", ["control"], ["shift"]) %>,
                    "to": <%= to([["down_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "git_gui", "jetbrains_ide", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Paste(Shift+Insert) for JIS keyboard",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("insert", ["shift"], ["any"]) %>,
                    "to": <%= to([["v", ["left_command"]]]) %>
                }
            ]
        },
        {
            "description": "PC-Style Paste(Shift+Fn) for ANSI keyboard",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("fn", ["shift"], ["any"]) %>,
                    "to": <%= to([["v", ["left_command"]]]) %>
                }
            ]
        },
        {
            "description": "Option(Alt)+Tab as Switch Application (Command+Tab)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("tab", ["option"], ["any"]) %>,
                    "to": <%= to([["tab", ["left_command"]]]) %>
                }
            ]
        },
        {
            "description": "PC-Style Control+Up/Down/Left/Right",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("left_arrow", ["control"], ["any"]) %>,
                    "to": <%= to([["left_arrow", ["left_option"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("right_arrow", ["control"], ["any"]) %>,
                    "to": <%= to([["right_arrow", ["left_option"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("up_arrow", ["control"], ["any"]) %>,
                    "to": <%= to([["up_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("down_arrow", ["control"], ["any"]) %>,
                    "to": <%= to([["down_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Copy/Paste/Cut",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("c", ["control"], ["any"]) %>,
                    "to": <%= to([["c", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("v", ["control"], ["any"]) %>,
                    "to": <%= to([["v", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("x", ["control"], ["any"]) %>,
                    "to": <%= to([["x", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Undo",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("z", ["control"], ["any"]) %>,
                    "to": <%= to([["z", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Redo",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("y", ["control"], ["any"]) %>,
                    "to": <%= to([["z", ["left_command", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Select-All",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("a", ["control"], ["any"]) %>,
                    "to": <%= to([["a", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Save",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("s", ["control"], ["any"]) %>,
                    "to": <%= to([["s", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style New",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("n", ["control"], ["any"]) %>,
                    "to": <%= to([["n", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Reload(F5, Ctrl+R)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("r", ["control"], ["any"]) %>,
                    "to": <%= to([["r", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("f5", [], ["any"]) %>,
                    "to": <%= to([["r", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style New Tab",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("t", ["control"], ["any"]) %>,
                    "to": <%= to([["t", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Find",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("f", ["control"], ["any"]) %>,
                    "to": <%= to([["f", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("g", ["control"], ["any"]) %>,
                    "to": <%= to([["g", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Open",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("o", ["control"], ["any"]) %>,
                    "to": <%= to([["o", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Bold/Italic/Underline(Ctrl+B/I/U)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("b", ["control"], ["any"]) %>,
                    "to": <%= to([["b", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("i", ["control"], ["any"]) %>,
                    "to": <%= to([["i", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("u", ["control"], ["any"]) %>,
                    "to": <%= to([["u", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Close Window",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("w", ["control"], ["any"]) %>,
                    "to": <%= to([["w", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Emoji Picker (Command+.)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("period", ["command"], []) %>,
                    "to": <%= to([["spacebar", ["left_control", "left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Spotlight Search (Command+S)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("s", ["command"], []) %>,
                    "to": <%= to([["spacebar", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Switch Input (Command+Space)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("spacebar", ["command"], []) %>,
                    "to": <%= to([["spacebar", ["left_control"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Lock Screen",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("l", ["command"], []) %>,
                    "to": <%= to([["power", ["left_control", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Screenshot to File (PrintScreen to select)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("print_screen", [], ["any"]) %>,
                    "to": <%= to([["4", ["left_command", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Screenshot to Clipboard (PrintScreen to select)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("print_screen", [], ["any"]) %>,
                    "to": <%= to([["4", ["left_command", "left_control", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Screenshot (PrintScreen for whole, Shift+PrintScreen to select)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("print_screen", ["shift"], ["any"]) %>,
                    "to": <%= to([["4", ["left_command", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("print_screen", [], ["any"]) %>,
                    "to": <%= to([["3", ["left_command", "left_shift"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Quit Application (Alt+F4 to Command+Q)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("f4", ["option"], []) %>,
                    "to": <%= to([["q", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "Command+E Opens Finder",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("e", ["command"], []) %>,
                    "to": [
                        { "shell_command": "open -a 'Finder.app'" }
                    ],
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "Control+Esc Opens Launchpad",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("escape", ["control"], []) %>,
                    "to": <%= to([["launchpad", []]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "Control+Shift+Esc Opens Activity Monitor",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("escape", ["control", "shift"], []) %>,
                    "to": [
                        { "shell_command": "open -a 'Activity Monitor.app'" }
                    ],
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Browser open location (Ctrl+L)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("l", ["control"], ["any"]) %>,
                    "to": <%= to([["l", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Back/Forward (Alt+Left Arrow/Alt+Right Arrow)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("left_arrow", ["option"], ["any"]) %>,
                    "to": <%= to([["left_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("right_arrow", ["option"], ["any"]) %>,
                    "to": <%= to([["right_arrow", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Browser Zoom (Ctrl+Plus/Minus/0)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("hyphen", ["control"], ["any"]) %>,
                    "to": <%= to([["hyphen", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("keypad_hyphen", ["control"], ["any"]) %>,
                    "to": <%= to([["keypad_hyphen", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("equal_sign", ["control"], ["any"]) %>,
                    "to": <%= to([["equal_sign", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("keypad_plus", ["control"], ["any"]) %>,
                    "to": <%= to([["keypad_plus", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("0", ["control"], ["any"]) %>,
                    "to": <%= to([["0", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                },
                {
                    "type": "basic",
                    "from": <%= from("keypad_0", ["control"], ["any"]) %>,
                    "to": <%= to([["keypad_0", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_if(["browser"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Control+Delete/Backspace",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("delete_or_backspace", ["control"], ["any"]) %>,
                    "to": <%= to([["delete_or_backspace", ["option"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        },
        {
            "description": "PC-Style Control+K",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("k", ["control"], ["any"]) %>,
                    "to": <%= to([["k", ["left_command"]]]) %>,
                    "conditions": [
                        <%= frontmost_application_unless(["remote_desktop", "virtual_machine", "terminal", "emacs"]) %>
                    ]
                }
            ]
        }
    ]
}
