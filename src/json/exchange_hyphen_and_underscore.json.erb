{
    "title": "Exchange hyphen and underscore",
    "rules": [
        {
            "description": "Exchange hyphen and underscore",
            "manipulators": [
                { "type": "basic", "from": <%= from("hyphen", [], ["caps_lock"]) %>, "to": <%= to([["hyphen", ["left_shift"]]]) %> },

                { "type": "basic", "from": <%= from("hyphen", ["shift"], ["caps_lock"]) %>, "to": <%= to([["hyphen"]]) %> }
            ]
        }
    ]
}
